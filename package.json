{
	"name": "vscode-pwa",
	"displayName": "PWA, VS Code Snippets",
	"description": "Progressive Web Apps Code Snippets. Let's build Progressive Web Apps in VS Code on the go!",
	"version": "0.2.1",
	"publisher": "mayeedwin",
	"engines": {
		"vscode": "^1.28.0"
	},
	"author": {
        "name": "Maye Edwin" 
    },

	"keywords": [
        "pwa",
        "progressive web apps",
		"service worker",
		"manifest",
		"workbox",
		"pwafire",
		"maye edwin"
	],

    "icon": ".vscode-pwa/icons/vscode-pwa.png",
    "galleryBanner": {
        "color": "#fafafa",
        "theme": "light"
    },
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "html",
				"path": "./snippets/html.json"
			},
			{
				"language": "json",
				"path": "./snippets/manifest.json"
			}
		]
	},
