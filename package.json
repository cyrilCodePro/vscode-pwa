{
	"name": "vscode-pwa",
	"displayName": "PWA, Progressive Web Apps",
	"description": "Progressive Web Apps Code Snippets. Let's build Progressive Web Apps in VS Code on the go!",
	"version": "0.0.6",
	"publisher": "mayeedwin",
	"engines": {
		"vscode": "^1.28.0"
	},
	"author": {
        "name": "Maye Edwin" 
    },

    "icon": ".vscode-pwa/icons/pwa.png",
    "galleryBanner": {
        "color": "#fafafa",
        "theme": "light"
    },
	"categories": [
		"Snippets"
	],
	"contributes": {
		"snippets": [
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "html",
				"path": "./snippets/html.json"
			},
			{
				"language": "json",
				"path": "./snippets/manifest.json"
			}
		]
	},
	"bugs": {
        "url": "https://github.com/mayeedwin/vscode-pwa/issues/new/choose",
        "email": "mayedwine1@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mayeedwin/vscode-pwa"
    },
    "homepage": "https://github.com/mayeedwin/vscode-pwa/blob/master/README.md"
}